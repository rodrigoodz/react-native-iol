{
  "expo": {
    "name": "IOL Unofficial APP",
    "slug": "react-native-iol",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#131e31"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#131e31"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-2009970378712355~6673226503"
      },
      "package": "com.rodrigoodz.reactnativeiol",
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
